generator client {
  provider = "prisma-client-js"
  output   = "./generated/authx"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserAuth {
  id        String    @id @default(cuid())
  email     String    @unique
  username  String    @unique
  password  String
  lastLogin DateTime?
}
